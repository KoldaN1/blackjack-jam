#include <iostream>
#include <cmath>

// Структура для ноды двусвязного списка
struct Node {
    int data;
    Node* next;
    Node* prev;
};

// Функция для проверки простого числа
bool isPrime(int num) {
    if (num <= 1) return false;
    for (int i = 2; i <= sqrt(num); i++) {
        if (num % i == 0) return false;
    }
    return true;
}

// Функция для создания нового узла
Node* createNode(int data) {
    Node* newNode = new Node();
    newNode->data = data;
    newNode->next = nullptr;
    newNode->prev = nullptr;
    return newNode;
}

// Функция для добавления узла в конец списка
void addNode(Node* head, int data) {
    Node* newNode = createNode(data);
    if (head->next == nullptr) {
        head->next = newNode;
        newNode->prev = head;
        newNode->next = head;
        head->prev = newNode;
    } else {
        Node* temp = head->prev;
        temp->next = newNode;
        newNode->prev = temp;
        newNode->next = head;
        head->prev = newNode;
    }
}

// Функция для удаления узла из списка
void removeNode(Node* head, Node* node) {
    if (node == head) return;
    if (node->next == head) {
        head->prev = node->prev;
        node->prev->next = head;
    } else {
        node->prev->next = node->next;
        node->next->prev = node->prev;
    }
    delete node;
}

// Функция для дублирования узла
void duplicateNode(Node* head, Node* node) {
    Node* newNode = createNode(node->data);
    if (node->next == head) {
        head->prev = newNode;
        newNode->next = head;
        newNode->prev = node;
        node->next = newNode;
    } else {
        newNode->next = node->next;
        newNode->prev = node;
        node->next->prev = newNode;
        node->next = newNode;
    }
}

// Функция для проверки упорядоченности списка
bool isOrdered(Node* head) {
    Node* temp = head->next;
    bool isAscending = true;
    bool isDescending = true;
    while (temp != head) {
        if (temp->data < temp->prev->data) isDescending = false;
        if (temp->data > temp->prev->data) isAscending = false;
        temp = temp->next;
    }
    return isAscending || isDescending;
}

// Функция для упорядочения списка по неубыванию первой цифры
void orderList(Node* head) {
    Node* temp = head->next;
    while (temp != head) {
        Node* nextTemp = temp->next;
        while (nextTemp != head) {
            if ((temp->data / 10) > (nextTemp->data / 10)) {
                int swap = temp->data;
                temp->data = nextTemp->data;
                nextTemp->data = swap;
            }
            nextTemp = nextTemp->next;
        }
        temp = temp->next;
    }
}

// Просто вывод
void printList(Node* head) {
    Node* temp = head->next;
    while (temp != head) {
        std::cout << temp->data << " ";
        temp = temp->next;
    }
    std::cout << std::endl;
}

int main() {
    Node* head = createNode(0);
    head->next = head;
    head->prev = head;

    int n;
    std::cout << "Write elements count: ";
    std::cin >> n;

    for (int i = 0; i < n; i++) {
        int data;
        std::cout << "Write element " << i + 1 << ": ";
        std::cin >> data;
        addNode(head, data);
    }

    if (isOrdered(head)) {
        Node* temp = head->next;
        while (temp != head) {
            Node* nextTemp = temp->next;
            if (isPrime(temp->data)) {
                removeNode(head, temp);
            } else if (temp->data % 10 == 0) {
                duplicateNode(head, temp);
            }
            temp = nextTemp;
        }
    } else {
        orderList(head);
    }

    printList(head);

    return 0;
}